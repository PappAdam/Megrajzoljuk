<Window x:Class="Rajzi.RunWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rajzi"
        mc:Ignorable="d"
        Title="Run" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="canvas" Background="#252323"></Canvas>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="4*"/>
            </Grid.ColumnDefinitions>
            <Rectangle Grid.Column="0" Height="auto" Width="auto" Fill="#EB9486"></Rectangle>
        </Grid>
    </Grid>
</Window>
